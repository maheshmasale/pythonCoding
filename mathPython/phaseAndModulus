
#https://www.hackerrank.com/challenges/polar-coordinates
import cmath
z =  '1+2j'
[print(round(i,3)) for i in cmath.polar(complex(input()))]

a = z.split('+')
x = a[0]
y = (a[1])[:len(a[1])-1]

print(abs(complex(int(x),int(y))))
print(cmath.phase(complex(int(x),int(y))))


